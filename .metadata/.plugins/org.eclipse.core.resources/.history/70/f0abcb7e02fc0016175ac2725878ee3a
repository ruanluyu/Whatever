package system;

import processing.core.PApplet;

public class GameAppRun extends PApplet{
	GameSystem gs ;
	public static void main(String[] args){
		GameAppRun app = new GameAppRun();
		
		app.runSketch();
		
	}
	@Override
	public void setup(){
		gs = new GameSystem(this);
		frameRate(60);
		textAlign();
	}
	public void draw(){
		gs.render();
		
		text("fps : "+ frameRate,0,0);
	}
	
	@Override
	public void settings(){
		size(800,400);
		
	}
}


