package system;

public class LineCrossMath {
	public static boolean isCross(float x1,float y1,float x2,float y2,float x3,float y3,float x4,float y4){
		float a = (y1 - y2) / (x1 - x2);  
		float b = (x1 * y2 - x2 * y1) / (x1 - x2);
		float c = (y3 - y4) / (x3 - x4);  
		float d = (x3 * y4 - x4 * y3) / (x3 - x4); 
		
		return false;
	}
}
